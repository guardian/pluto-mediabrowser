{
  "name": "pluto-mediabrowser",
  "version": "1.0.0",
  "description": "GNM's basic media browsing interface for VS",
  "main": "index.js",
  "scripts": {
    "test": "jest --verbose",
    "dev": "webpack -d --watch",
    "compile": "webpack -p",
    "snyk-protect": "snyk protect",
    "lint": "prettier --check app __tests__",
    "lint:fix": "prettier --write app __tests__",
    "update-interfaces": "ts-interface-builder app/vidispine/search/VidispineSearch.ts app/vidispine/item/VidispineItem.ts app/vidispine/field-group/CustomData.ts app/vidispine/field-group/VidispineFieldGroup.ts"
  },
  "jest": {
    "moduleFileExtensions": [
      "jsx",
      "js",
      "tsx",
      "ts",
      "json"
    ],
    "verbose": true,
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "testResultsProcessor": "jest-junit",
    "testEnvironment": "jsdom",
    "setupFiles": [
      "./jestSetup.jsx"
    ],
    "transform": {
      "^.+\\.[jt]sx?$": "ts-jest"
    }
  },
  "author": "Andy Gallagher <andy.gallagher@theguardian.com>",
  "devDependencies": {
    "@svgr/webpack": "^5.4.0",
    "@types/enzyme": "^3.10.5",
    "@types/jest": "^26.0.7",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/react": "^16.9.36",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/sinon": "^9.0.4",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "expect": "^24.9.0",
    "husky": ">=4",
    "jest": "^26.0.1",
    "jest-cli": "^24.8.0",
    "jest-fetch-mock": "^3.0.1",
    "jest-junit": "^4.0.0",
    "moxios": "^0.4.0",
    "prettier": "^2.0.5",
    "react-test-renderer": "^16.12.0",
    "sass": "^1.26.9",
    "sass-loader": "^8.0.2",
    "sinon": "^2.3.7",
    "source-map-loader": "^1.0.0",
    "terser-webpack-plugin": "^1.4.3",
    "ts-interface-builder": "^0.2.2",
    "ts-jest": "^26.1.4",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "@date-io/core": "^1.3.6",
    "@date-io/date-fns": "^1.3.6",
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-brands-svg-icons": "^5.12.0",
    "@fortawesome/free-regular-svg-icons": "^5.12.0",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "@material-ui/core": "^4.10.2",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@material-ui/pickers": "^3.2.10",
    "@types/react-helmet": "^6.1.0",
    "axios": "^0.19.2",
    "cross-storage": "^1.0.0",
    "css-loader": "^2.1.1",
    "date-fns": "^2.15.0",
    "identity-obj-proxy": "^3.0.0",
    "jsonwebtoken": "^8.5.1",
    "material-ui-chip-input": "^1.1.0",
    "moment": "^2.20.1",
    "moment-duration-format": "2.2.2",
    "moment-timezone": "^0.5.29",
    "pluto-headers": "https://gitlab.com/codmill/customer-projects/guardian/pluto-headers.git#master",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-select": "^3.1.0",
    "react-tooltip": "^3.11.2",
    "reactjs-popup": "^1.5.0",
    "recharts": "^1.8.5",
    "style-loader": "^0.20.2",
    "ts-interface-checker": "^0.1.11",
    "url-loader": "^4.1.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "yarn": "^1.21.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,md}": "prettier --write"
  },
  "license": "ISC"
}
